{
  "proto": {
    "id": "?s",
    "metadata": {
    	"property": "?p",
    	"value": "?o"
    }
  },
  "$where": [
    "?s a dcat:Dataset",
    "?s dct:spatial ?spatial",
    "?s dct:publisher ?publisher",
    "?s rdfs:label ?label",
    "?spatial rdfs:labelx ?geoLabel",
    "?s ?p ?o"
  ],
  "$filter": "regex(?label, ?_keyword, 'i') || regex(?geoLabel, ?_keyword, 'i')",
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#"
  },
  "$orderby": "?id",
  "$lang": "en",
  "$limit": 50,
  "grlc": {
    "summary": "Retrieve all information about CYBELE datasets having the provided keyword, e.g., test. The query checks both, the dataset label and any geolocation label associated (e.g., Greece)",
    "endpoint": "https://www.foodie-cloud.org/sparql",
    "tags": [ "test" ],
    "method": "GET",
    "pagination": 50
  }
}